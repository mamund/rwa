<alps>
 <descriptor id="VideoObject" type="semantic" href="http://alps.io/schema.org/MediaObject">
  <doc format="html">
   A video file.
  </doc>
    <descriptor id="about" type="semantic" href="http://alps.io/schema.org/CreativeWork#about" rt="http://alps.io/schema.org/Thing">
   <doc format="html">
    The subject matter of the content.
   </doc>
  </descriptor>

  <descriptor id="accountablePerson" type="semantic" href="http://alps.io/schema.org/CreativeWork#accountablePerson" rt="http://alps.io/schema.org/Person">
   <doc format="html">
    Specifies the Person that is legally accountable for the CreativeWork.
   </doc>
  </descriptor>

  <descriptor id="additionalType" type="semantic" href="http://alps.io/schema.org/Thing#additionalType" rt="http://alps.io/schema.org/URL">
   <doc format="html">
    An additional type for the item, typically used for adding more specific types from external vocabularies in microdata syntax. This is a relationship between something and a class that the thing is in. In RDFa syntax, it is better to use the native RDFa syntax - the 'typeof' attribute - for multiple types. Schema.org tools may have only weaker understanding of extra types, in particular those defined externally.
   </doc>
  </descriptor>

  <descriptor id="aggregateRating" type="semantic" href="http://alps.io/schema.org/CreativeWork#aggregateRating" rt="http://alps.io/schema.org/AggregateRating">
   <doc format="html">
    The overall rating, based on a collection of reviews or ratings, of the item.
   </doc>
  </descriptor>

  <descriptor id="alternativeHeadline" type="semantic" href="http://alps.io/schema.org/CreativeWork#alternativeHeadline" rt="http://alps.io/schema.org/Text">
   <doc format="html">
    A secondary title of the CreativeWork.
   </doc>
  </descriptor>

  <descriptor id="associatedArticle" type="semantic" href="http://alps.io/schema.org/MediaObject#associatedArticle" rt="http://alps.io/schema.org/NewsArticle">
   <doc format="html">
    A NewsArticle associated with the Media Object.
   </doc>
  </descriptor>

  <descriptor id="associatedMedia" type="semantic" href="http://alps.io/schema.org/CreativeWork#associatedMedia" rt="http://alps.io/schema.org/MediaObject">
   <doc format="html">
    The media objects that encode this creative work. This property is a synonym for encodings.
   </doc>
  </descriptor>

  <descriptor id="audience" type="semantic" href="http://alps.io/schema.org/CreativeWork#audience" rt="http://alps.io/schema.org/Audience">
   <doc format="html">
    The intended audience of the work, i.e. the group for whom the work was created.
   </doc>
  </descriptor>

  <descriptor id="audio" type="semantic" href="http://alps.io/schema.org/CreativeWork#audio" rt="http://alps.io/schema.org/AudioObject">
   <doc format="html">
    An embedded audio object.
   </doc>
  </descriptor>

  <descriptor id="author" type="semantic" href="http://alps.io/schema.org/CreativeWork#author" rt="http://alps.io/schema.org/Organization">
   <doc format="html">
    The author of this content. Please note that author is special in that HTML 5 provides a special mechanism for indicating authorship via the rel tag. That is equivalent to this and may be used interchangeably.
   </doc>
  </descriptor>

  <descriptor id="award" type="semantic" href="http://alps.io/schema.org/CreativeWork#award" rt="http://alps.io/schema.org/Text">
   <doc format="html">
    An award won by this person or for this creative work.
   </doc>
  </descriptor>

  <descriptor id="awards" type="semantic" href="http://alps.io/schema.org/CreativeWork#awards" rt="http://alps.io/schema.org/Text">
   <doc format="html">
    Awards won by this person or for this creative work. (legacy spelling; see singular form, award)
   </doc>
  </descriptor>

  <descriptor id="bitrate" type="semantic" href="http://alps.io/schema.org/MediaObject#bitrate" rt="http://alps.io/schema.org/Text">
   <doc format="html">
    The bitrate of the media object.
   </doc>
  </descriptor>

  <descriptor id="comment" type="semantic" href="http://alps.io/schema.org/CreativeWork#comment" rt="http://alps.io/schema.org/UserComments">
   <doc format="html">
    Comments, typically from users, on this CreativeWork.
   </doc>
  </descriptor>

  <descriptor id="contentLocation" type="semantic" href="http://alps.io/schema.org/CreativeWork#contentLocation" rt="http://alps.io/schema.org/Place">
   <doc format="html">
    The location of the content.
   </doc>
  </descriptor>

  <descriptor id="contentRating" type="semantic" href="http://alps.io/schema.org/CreativeWork#contentRating" rt="http://alps.io/schema.org/Text">
   <doc format="html">
    Official rating of a piece of content&#x2014;for example,'MPAA PG-13'.
   </doc>
  </descriptor>

  <descriptor id="contentSize" type="semantic" href="http://alps.io/schema.org/MediaObject#contentSize" rt="http://alps.io/schema.org/Text">
   <doc format="html">
    File size in (mega/kilo) bytes.
   </doc>
  </descriptor>

  <descriptor id="contentUrl" type="semantic" href="http://alps.io/schema.org/MediaObject#contentUrl" rt="http://alps.io/schema.org/URL">
   <doc format="html">
    Actual bytes of the media object, for example the image file or video file. (previous spelling: contentURL)
   </doc>
  </descriptor>

  <descriptor id="contributor" type="semantic" href="http://alps.io/schema.org/CreativeWork#contributor" rt="http://alps.io/schema.org/Organization">
   <doc format="html">
    A secondary contributor to the CreativeWork.
   </doc>
  </descriptor>

  <descriptor id="copyrightHolder" type="semantic" href="http://alps.io/schema.org/CreativeWork#copyrightHolder" rt="http://alps.io/schema.org/Organization">
   <doc format="html">
    The party holding the legal copyright to the CreativeWork.
   </doc>
  </descriptor>

  <descriptor id="copyrightYear" type="semantic" href="http://alps.io/schema.org/CreativeWork#copyrightYear" rt="http://alps.io/schema.org/Number">
   <doc format="html">
    The year during which the claimed copyright for the CreativeWork was first asserted.
   </doc>
  </descriptor>

  <descriptor id="creator" type="semantic" href="http://alps.io/schema.org/CreativeWork#creator" rt="http://alps.io/schema.org/Organization">
   <doc format="html">
    The creator/author of this CreativeWork or UserComments. This is the same as the Author property for CreativeWork.
   </doc>
  </descriptor>

  <descriptor id="dateCreated" type="semantic" href="http://alps.io/schema.org/CreativeWork#dateCreated" rt="http://alps.io/schema.org/Date">
   <doc format="html">
    The date on which the CreativeWork was created.
   </doc>
  </descriptor>

  <descriptor id="dateModified" type="semantic" href="http://alps.io/schema.org/CreativeWork#dateModified" rt="http://alps.io/schema.org/Date">
   <doc format="html">
    The date on which the CreativeWork was most recently modified.
   </doc>
  </descriptor>

  <descriptor id="datePublished" type="semantic" href="http://alps.io/schema.org/CreativeWork#datePublished" rt="http://alps.io/schema.org/Date">
   <doc format="html">
    Date of first broadcast/publication.
   </doc>
  </descriptor>

  <descriptor id="description" type="semantic" href="http://alps.io/schema.org/Thing#description" rt="http://alps.io/schema.org/Text">
   <doc format="html">
    A short description of the item.
   </doc>
  </descriptor>

  <descriptor id="discussionUrl" type="semantic" href="http://alps.io/schema.org/CreativeWork#discussionUrl" rt="http://alps.io/schema.org/URL">
   <doc format="html">
    A link to the page containing the comments of the CreativeWork.
   </doc>
  </descriptor>

  <descriptor id="duration" type="semantic" href="http://alps.io/schema.org/MediaObject#duration" rt="http://alps.io/schema.org/Duration">
   <doc format="html">
    The duration of the item (movie, audio recording, event, etc.) in <a href='http://en.wikipedia.org/wiki/ISO_8601'>ISO 8601 date format</a>.
   </doc>
  </descriptor>

  <descriptor id="editor" type="semantic" href="http://alps.io/schema.org/CreativeWork#editor" rt="http://alps.io/schema.org/Person">
   <doc format="html">
    Specifies the Person who edited the CreativeWork.
   </doc>
  </descriptor>

  <descriptor id="embedUrl" type="semantic" href="http://alps.io/schema.org/MediaObject#embedUrl" rt="http://alps.io/schema.org/URL">
   <doc format="html">
    A URL pointing to a player for a specific video. In general, this is the information in the <code>src</code> element of an <code>embed</code> tag and should not be the same as the content of the <code>loc</code> tag. (previous spelling: embedURL)
   </doc>
  </descriptor>

  <descriptor id="encodesCreativeWork" type="semantic" href="http://alps.io/schema.org/MediaObject#encodesCreativeWork" rt="http://alps.io/schema.org/CreativeWork">
   <doc format="html">
    The creative work encoded by this media object
   </doc>
  </descriptor>

  <descriptor id="encoding" type="semantic" href="http://alps.io/schema.org/CreativeWork#encoding" rt="http://alps.io/schema.org/MediaObject">
   <doc format="html">
    A media object that encode this CreativeWork.
   </doc>
  </descriptor>

  <descriptor id="encodingFormat" type="semantic" href="http://alps.io/schema.org/MediaObject#encodingFormat" rt="http://alps.io/schema.org/Text">
   <doc format="html">
    mp3, mpeg4, etc.
   </doc>
  </descriptor>

  <descriptor id="encodings" type="semantic" href="http://alps.io/schema.org/CreativeWork#encodings" rt="http://alps.io/schema.org/MediaObject">
   <doc format="html">
    The media objects that encode this creative work (legacy spelling; see singular form, encoding).
   </doc>
  </descriptor>

  <descriptor id="expires" type="semantic" href="http://alps.io/schema.org/MediaObject#expires" rt="http://alps.io/schema.org/Date">
   <doc format="html">
    Date the content expires and is no longer useful or available. Useful for videos.
   </doc>
  </descriptor>

  <descriptor id="genre" type="semantic" href="http://alps.io/schema.org/CreativeWork#genre" rt="http://alps.io/schema.org/Text">
   <doc format="html">
    Genre of the creative work
   </doc>
  </descriptor>

  <descriptor id="headline" type="semantic" href="http://alps.io/schema.org/CreativeWork#headline" rt="http://alps.io/schema.org/Text">
   <doc format="html">
    Headline of the article
   </doc>
  </descriptor>

  <descriptor id="height" type="semantic" href="http://alps.io/schema.org/MediaObject#height" rt="http://alps.io/schema.org/Distance">
   <doc format="html">
    The height of the item.
   </doc>
  </descriptor>

  <descriptor id="image" type="semantic" href="http://alps.io/schema.org/Thing#image" rt="http://alps.io/schema.org/URL">
   <doc format="html">
    URL of an image of the item.
   </doc>
  </descriptor>

  <descriptor id="inLanguage" type="semantic" href="http://alps.io/schema.org/CreativeWork#inLanguage" rt="http://alps.io/schema.org/Text">
   <doc format="html">
    The language of the content. please use one of the language codes from the <a href='http://tools.ietf.org/html/bcp47'>IETF BCP 47 standard.</a>
   </doc>
  </descriptor>

  <descriptor id="interactionCount" type="semantic" href="http://alps.io/schema.org/CreativeWork#interactionCount" rt="http://alps.io/schema.org/Text">
   <doc format="html">
    A count of a specific user interactions with this item&#x2014;for example, <code>20 UserLikes</code>, <code>5 UserComments</code>, or <code>300 UserDownloads</code>. The user interaction type should be one of the sub types of <a href='UserInteraction'>UserInteraction</a>.
   </doc>
  </descriptor>

  <descriptor id="isFamilyFriendly" type="semantic" href="http://alps.io/schema.org/CreativeWork#isFamilyFriendly" rt="http://alps.io/schema.org/Boolean">
   <doc format="html">
    Indicates whether this content is family friendly.
   </doc>
  </descriptor>

  <descriptor id="keywords" type="semantic" href="http://alps.io/schema.org/CreativeWork#keywords" rt="http://alps.io/schema.org/Text">
   <doc format="html">
    The keywords/tags used to describe this content.
   </doc>
  </descriptor>

  <descriptor id="mentions" type="semantic" href="http://alps.io/schema.org/CreativeWork#mentions" rt="http://alps.io/schema.org/Thing">
   <doc format="html">
    Indicates that the CreativeWork contains a reference to, but is not necessarily about a concept.
   </doc>
  </descriptor>

  <descriptor id="name" type="semantic" href="http://alps.io/schema.org/Thing#name" rt="http://alps.io/schema.org/Text">
   <doc format="html">
    The name of the item.
   </doc>
  </descriptor>

  <descriptor id="offers" type="semantic" href="http://alps.io/schema.org/CreativeWork#offers" rt="http://alps.io/schema.org/Offer">
   <doc format="html">
    An offer to sell this item&#x2014;for example, an offer to sell a product, the DVD of a movie, or tickets to an event.
   </doc>
  </descriptor>

  <descriptor id="playerType" type="semantic" href="http://alps.io/schema.org/MediaObject#playerType" rt="http://alps.io/schema.org/Text">
   <doc format="html">
    Player type required&#x2014;for example, Flash or Silverlight.
   </doc>
  </descriptor>

  <descriptor id="provider" type="semantic" href="http://alps.io/schema.org/CreativeWork#provider" rt="http://alps.io/schema.org/Organization">
   <doc format="html">
    Specifies the Person or Organization that distributed the CreativeWork.
   </doc>
  </descriptor>

  <descriptor id="publisher" type="semantic" href="http://alps.io/schema.org/CreativeWork#publisher" rt="http://alps.io/schema.org/Organization">
   <doc format="html">
    The publisher of the creative work.
   </doc>
  </descriptor>

  <descriptor id="publishingPrinciples" type="semantic" href="http://alps.io/schema.org/CreativeWork#publishingPrinciples" rt="http://alps.io/schema.org/URL">
   <doc format="html">
    Link to page describing the editorial principles of the organization primarily responsible for the creation of the CreativeWork.
   </doc>
  </descriptor>

  <descriptor id="regionsAllowed" type="semantic" href="http://alps.io/schema.org/MediaObject#regionsAllowed" rt="http://alps.io/schema.org/Place">
   <doc format="html">
    The regions where the media is allowed. If not specified, then it's assumed to be allowed everywhere. Specify the countries in <a href='http://en.wikipedia.org/wiki/ISO_3166'>ISO 3166 format</a>.
   </doc>
  </descriptor>

  <descriptor id="requiresSubscription" type="semantic" href="http://alps.io/schema.org/MediaObject#requiresSubscription" rt="http://alps.io/schema.org/Boolean">
   <doc format="html">
    Indicates if use of the media require a subscription  (either paid or free). Allowed values are <code>true</code> or <code>false</code> (note that an earlier version had 'yes', 'no').
   </doc>
  </descriptor>

  <descriptor id="review" type="semantic" href="http://alps.io/schema.org/CreativeWork#review" rt="http://alps.io/schema.org/Review">
   <doc format="html">
    A review of the item.
   </doc>
  </descriptor>

  <descriptor id="reviews" type="semantic" href="http://alps.io/schema.org/CreativeWork#reviews" rt="http://alps.io/schema.org/Review">
   <doc format="html">
    Review of the item (legacy spelling; see singular form, review).
   </doc>
  </descriptor>

  <descriptor id="sourceOrganization" type="semantic" href="http://alps.io/schema.org/CreativeWork#sourceOrganization" rt="http://alps.io/schema.org/Organization">
   <doc format="html">
    The Organization on whose behalf the creator was working.
   </doc>
  </descriptor>

  <descriptor id="text" type="semantic" href="http://alps.io/schema.org/CreativeWork#text" rt="http://alps.io/schema.org/Text">
   <doc format="html">
    The textual content of this CreativeWork.
   </doc>
  </descriptor>

  <descriptor id="thumbnailUrl" type="semantic" href="http://alps.io/schema.org/CreativeWork#thumbnailUrl" rt="http://alps.io/schema.org/URL">
   <doc format="html">
    A thumbnail image relevant to the Thing.
   </doc>
  </descriptor>

  <descriptor id="uploadDate" type="semantic" href="http://alps.io/schema.org/MediaObject#uploadDate" rt="http://alps.io/schema.org/Date">
   <doc format="html">
    Date when this media object was uploaded to this site.
   </doc>
  </descriptor>

  <descriptor id="url" type="semantic" href="http://alps.io/schema.org/Thing#url" rt="http://alps.io/schema.org/URL">
   <doc format="html">
    URL of the item.
   </doc>
  </descriptor>

  <descriptor id="version" type="semantic" href="http://alps.io/schema.org/CreativeWork#version" rt="http://alps.io/schema.org/Number">
   <doc format="html">
    The version of the CreativeWork embodied by a specified resource.
   </doc>
  </descriptor>

  <descriptor id="video" type="semantic" href="http://alps.io/schema.org/CreativeWork#video" rt="http://alps.io/schema.org/VideoObject">
   <doc format="html">
    An embedded video object.
   </doc>
  </descriptor>

  <descriptor id="videoFrameSize" type="semantic" rt="http://alps.io/schema.org/Text">
   <doc format="html">
    The frame size of the video.
   </doc>
  </descriptor>

  <descriptor id="videoQuality" type="semantic" rt="http://alps.io/schema.org/Text">
   <doc format="html">
    The quality of the video.
   </doc>
  </descriptor>

  <descriptor id="width" type="semantic" href="http://alps.io/schema.org/MediaObject#width" rt="http://alps.io/schema.org/Distance">
   <doc format="html">
    The width of the item.
   </doc>
  </descriptor>

 </descriptor>
</alps>